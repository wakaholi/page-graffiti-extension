{
    "manifest_version": 2,
    "name": "page-graffiti-extension",
    "description": "ページに落書きできるようにする拡張です",
    "version": "0.0.1",
    "icons": { "128": "image/icon128.png" },
    "content_scripts": [
      {
          "matches": [ "http://*/*", "https://*/*" ],
          "js": [ "js/graffiti.js" ]
      }
  ],
    "permissions": ["contextMenus", "tabs", "activeTab", "https://*/"]
  }